#ifndef _CHEVENT_HPP_
#define _CHEVENT_HPP_

#include <vector>
#include <TObject.h>

const int RECORD_LENGTH=4096;

class TChEvent : public TObject{
public:
  TChEvent();
  int baseline;
  std::vector<uint16_t> data;
  uint16_t Clk[RECORD_LENGTH];
  int adc;
  double energy;

  void Analyze();
  void Clear();
  
  ClassDef(TChEvent,1);
};

#endif
